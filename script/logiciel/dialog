#exemple utilisation dialog avec choix pour une selection d'option et interface=$ pour affecter une valeur a une variable
choix=$(dialog --clear --backtitle "?" \
    --title "?" \
    --menu "?" 15 50 2 \
    1 "?" \
    2 "?" \
    2>&1 >/dev/tty)
  interface=$(dialog --inputbox "?" 8 50 2>&1 >/dev/tty)
